<!--
    XAML styling,
    commonly used colors
    
    #46b6ac - teal
    #0b2c29 - darkblue
    #162d35 - blue
-->
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:alpha">



    <Color x:Key="Primary">#46b6ac</Color>
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}"></SolidColorBrush>
    <Color x:Key="Secondary">#162d35</Color>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource Secondary}"></SolidColorBrush>
</ResourceDictionary>